<div class="block">
  <div class="secondary-navigation">
    <ul class="wat-cf">
      <li class="first"><%= link_to "#{t("web-app-theme.list", :default => "Listar")}", cadclis_path  %></li>
      <li><%= link_to "#{t("web-app-theme.new", :default => "Novo")}", new_cadcli_path %></li>
      <li class="active"><%= link_to "#{t("web-app-theme.edit", :default => "Editar")}", edit_cadcli_path %></li>
    </ul>
  </div>          
  <div class="content">            
    <h2 class="title"><%= t("web-app-theme.edit", :default => "Edit") %> Cadcli</h2>
    <div class="inner">
      <%= form_for @cadcli, :url => cadcli_path(@cadcli), :html => { :class => :form } do |f| -%>
      <%= render :partial => "form", :locals => {:f => f} %>
     
 
      <% end -%>
    </div>
  </div>
</div>

<% content_for :sidebar, render(:partial => 'sidebar') -%>