
<div class="block">
  <div class="secondary-navigation">
    <ul class="wat-cf">
      <li class="first"><%= link_to "#{t("web-app-theme.list", :default => "Listar")}", cad_dns_path  %></li>
      <li><%= link_to "#{t("web-app-theme.new", :default => "Novo")}", new_cad_dn_path %></li>
      <li class="active"><%= link_to "#{t("web-app-theme.show", :default => "Visualizar")}", cad_dn_path %></li>
    </ul>
  </div>          
  <div class="content">
    <div class="inner">
      
        <p>
          <b><%= t("activerecord.attributes.cad_dns.email", :default => t("activerecord.labels.email", :default => "Email:")) %>:</b>
          <%= @cad_dn.email %>
        </p>

        <p>
          <b><%= t("activerecord.attributes.cad_dns.senha", :default => t("activerecord.labels.senha", :default => "Senha")) %>:</b>
          <%= @cad_dn.senha %>
        </p>

        <p>
          <b><%= t("activerecord.attributes.cad_dns.cliente", :default => t("activerecord.labels.valunit", :default => "Cliente")) %>:</b>
          <%= @cad_dn.cliente %>
        </p>

         <p>
          <b><%= t("activerecord.attributes.cad_dns.dns", :default => t("activerecord.labels.dns", :default => "DNS")) %>:</b>
          <%= @cad_dn.dns %>
        </p>
        
      <div class="wat-cf">
        <%= link_to image_tag("web-app-theme/icons/application_edit.png", :alt => "#{t("web-app-theme.edit", :default=> "Editar")}") + " " + t("web-app-theme.edit", :default=> "Edit"), edit_cad_dn_path(@cad_dn), :class => "button" %> 
        <%= link_to image_tag("web-app-theme/icons/cross.png", :alt => "#{t("web-app-theme.delete", :default=> "Delete")}") + " " + t("web-app-theme.delete", :default => "Delete"), cad_dn_path(@cad_dn), :method => "delete", :class => "button", :confirm => "#{t("web-app-theme.confirm", :default => "Voce tem certeza?")}" %>
      </div>      
    </div>
  </div>
</div>

<% content_for :sidebar, render(:partial => 'sidebar') -%>






