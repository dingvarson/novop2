<div class="block">
  <div class="secondary-navigation">
    <ul class="wat-cf">
    <li class="active"><%= link_to "#{t("web-app-theme.new", :default => "Novo Email")}", new_contact_path %></li>
    </ul>
  </div>  
       
  <div class="content">            
    <h2 class="title"><%= t("web-app-theme.new", :default => "Envio de Emails")%></h2>
    <div class="inner">
      <%= form_for :contact, :url => contacts_path, :html => { :class => :form } do |f| -%>
        <%= render :partial => "form", :locals => {:f => f} %>
      <% end -%>
    </div>
  </div>
</div>

<% content_for :sidebar, render(:partial => 'sidebar') -%>


