<div class="block">
  <div class="secondary-navigation">
    <ul class="wat-cf">
      <li class="first"><%= link_to "#{t("web-app-theme.list", :default => "Listar")}", orders_path  %></li>
      <li><%= link_to "#{t("web-app-theme.new", :default => "Novo")}", new_order_path %></li>
      <li class="active"><%= link_to "#{t("web-app-theme.edit", :default => "Editar")}", edit_order_path %></li>
    </ul>
  </div>          
  <div class="content">            
    <h2 class="title"><%= t("web-app-theme.edit", :default => "Editar O.S") %></h2>
    <div class="inner">
      <%= form_for @order, :url => order_path(@order), :html => { :class => :form } do |f| -%>
        <%= render :partial => "form", :locals => {:f => f} %>
      <% end -%>
    </div>
  </div>
</div>

