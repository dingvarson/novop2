<h1>Listing items</h1>

<table>
  <tr>
    <th>Quant</th>
    <th>Descr</th>
    <th>Val unit</th>
    <th>Val total</th>
    <th>Order</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @items.each do |item| %>
  <tr>
    <td><%= item.quant %></td>
    <td><%= item.descr %></td>
    <td><%= item.val_unit %></td>
    <td><%= item.val_total %></td>
    <td><%= item.order %></td>
    <td><%= link_to 'Show', item %></td>
    <td><%= link_to 'Edit', edit_item_path(item) %></td>
    <td><%= link_to 'Destroy', item, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Item', new_item_path %>
